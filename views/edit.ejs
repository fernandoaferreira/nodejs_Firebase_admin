<%- include('./layout/head') %>

<div class="container">
<form action="/edit/<%= id %>" method="POST">
 <div class="form-group">
    <label for="">Nome</label>
    <input type="text" name="nome" id="nome" class="form-control" placeholder="Digite o nome" value="<%= contatos.nome %>">
  </div>
  <div class="form-group">
  <label for="">Email</label>
  <input type="text" name="email" id="email" class="form-control" placeholder="Digite o email" value="<%= contatos.email %>">
</div>
  <button type="submit" class="btn btn-danger">Salvar</button>
</form>
<p></p>
<a href='/ver/<%= id %>'><button class="btn btn-primary">Voltar</button></a>
</div>

<%- include('./layout/footer') %>